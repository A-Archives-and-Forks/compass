{
  "name": "@mongodb-js/compass-workspace",
  "version": "0.0.1",
  "description": "React Components used in Compass",
  "license": "SSPL",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "bootstrap": "npm run compile",
    "check": "npm run lint && npm run depcheck",
    "clean": "rimraf lib",
    "precompile": "npm run clean",
    "compile": "npm run compile-ts",
    "compile-ts": "webpack --config ./webpack.config.js",
    "cover": "nyc npm run test",
    "depcheck": "depcheck",
    "lint": "eslint src/**/*.tsx src/**/*.ts",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "test": "npm run lint && echo \"NO TESTS :)\"",
    "test-check-ci": "npm run check && npm test",
    "test-ci": "npm run test"
  },
  "dependencies": {
    "@leafygreen-ui/icon": "^11.2.0",
    "@leafygreen-ui/icon-button": "^9.1.5",
    "flexlayout-react": "v0.5.12"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.25.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "nyc": "^15.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.2.0",
    "typescript": "^4.3.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
